    "brotli-size": "^4.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "glob": "^11.0.0",
    "gzip-size": "^6.0.0",
    "html-webpack-plugin": "^5.6.0",
    "kleur": "^4.1.5",
    "prettier": "^3.3.3",
    "pretty-bytes": "^5.6.0",
    "style-loader": "^4.0.0",
    "ts-node": "~10.9.2",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
